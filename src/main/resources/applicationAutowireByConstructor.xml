<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--  bean definitions here  -->
    <bean id="specification" class="car.example.Autowire.Constuctor.Specification">
        <property name="make" value="Toyota"/>
        <property name="model" value="Corolla"/>
    </bean>
    <bean id="myCar" class="car.example.Autowire.Constuctor.Car" autowire="constructor"/>

    <!--    by constructor will search the  only one matching bean for specific bean -> in this case it is specification-->
</beans>